{
  "name": "collective-ai-tools",
  "version": "1.0.0",
  "description": "A comprehensive platform featuring both curated external AI tools and powerful AI workspace tools for developers, researchers, and enthusiasts.",
  "type": "module",
  "license": "MIT",
  "author": "Collective AI Tools Team",
  "homepage": "https://collectiveai.tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hyraze/collective-ai-tools.git"
  },
  "bugs": {
    "url": "https://github.com/Hyraze/collective-ai-tools/issues"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "tools",
    "automation",
    "workflow",
    "agents",
    "mcp",
    "n8n",
    "code-review",
    "summarization",
    "react",
    "typescript",
    "open-source"
  ],
  "scripts": {
    "dev": "vite",
    "dev:local": "concurrently \"pnpm run dev:api\" \"vite --config vite.config.dev.ts\"",
    "dev:api": "node server.js",
    "prebuild": "cp README.md public/",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "security:check": "node scripts/security-check.js",
    "security:audit": "pnpm audit --audit-level moderate",
    "deps:check": "pnpm outdated",
    "deps:update": "pnpm update --latest && pnpm audit --fix",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "vercel": "^48.1.6",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "@vercel/node": "^5.3.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.3",
    "tailwind-merge": "^3.3.1"
  }
}
